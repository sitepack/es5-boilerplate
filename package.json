{
  "name": "router5-boilerplate",
  "version": "0.3.0",
  "description": "Sitepack boilerplate with router5, minimal config.",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development node-dev node_modules/@sitepack/dev-server",
    "dev:node": "NODE_ENV=development node node_modules/@sitepack/dev-server",
    "webpack:prerender": "NODE_ENV=production webpack --config ./webpack/config.prerender.js",
    "prerender": "node node_modules/@sitepack/prerender",
    "webpack:prod": "NODE_ENV=production webpack --config ./webpack/config.prod.js",
    "prebuild": "rimraf dist",
    "build": "npm run webpack:prerender && npm run prerender && npm run webpack:prod",
    "postbuild": "rimraf ./dist/assets/render.js ./dist/assets/prerender.js ./dist/assets/prerender.css  ./dist/.gitkeep",
    "start": "NODE_ENV=production node node_modules/@sitepack/static-server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sitepack/es5-boilerplate.git"
  },
  "author": "Jas Chen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sitepack/es5-boilerplate/issues"
  },
  "devDependencies": {
    "@sitepack/dev-server": "^0.5.0",
    "@sitepack/helper": "^0.5.0",
    "@sitepack/prerender": "^0.5.0",
    "@sitepack/remove-link-element": "^1.0.0",
    "@sitepack/route-loader": "^0.3.0",
    "@sitepack/static-server": "^0.1.1",
    "bundle-loader": "^0.5.4",
    "copy-webpack-plugin": "^0.2.0",
    "css-loader": "^0.23.0",
    "extract-text-webpack-plugin": "^0.9.0",
    "node-dev": "^2.7.1",
    "rimraf": "^2.4.4",
    "router5": "^1.1.0",
    "router5-history": "^1.0.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.8"
  }
}
