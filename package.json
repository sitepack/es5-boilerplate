{
  "name": "router5-boilerplate",
  "version": "0.2.2",
  "description": "Sitepack boilerplate with router5, minimal config.",
  "main": "index.js",
  "scripts": {
    "dev": "node-dev node_modules/@sitepack/dev-server",
    "dev:node": "node node_modules/@sitepack/dev-server",
    "extractCss": "webpack --config ./webpack/config.prod.exCss.js",
    "prerender": "node node_modules/@sitepack/prerender",
    "webpack": "webpack --config ./webpack/config.prod.final.js",
    "prebuild": "rimraf dist ./routeModules.js",
    "build": "npm run extractCss && npm run prerender && npm run webpack",
    "postbuild": "rimraf ./dist/assets/layout.js ./routeModules.js",
    "start": "node node_modules/@sitepack/static-server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sitepack/es5-boilerplate.git"
  },
  "author": "Jas Chen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sitepack/es5-boilerplate/issues"
  },
  "devDependencies": {
    "@sitepack/dev-server": "^0.1.1",
    "@sitepack/helper": "^0.3.0",
    "@sitepack/prerender": "^0.1.0",
    "@sitepack/route-loader": "^0.1.2",
    "@sitepack/static-server": "^0.1.1",
    "bundle-loader": "^0.5.4",
    "copy-webpack-plugin": "^0.2.0",
    "css-loader": "^0.21.0",
    "extract-text-webpack-plugin": "^0.9.0",
    "node-dev": "^2.7.1",
    "rimraf": "^2.4.3",
    "router5": "^1.0.0",
    "router5-history": "^1.0.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.2"
  }
}
